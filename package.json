{
  "name": "ol-toolbar-ui",
  "version": "1.0.3",
  "description": "一个基于 OpenLayers 和 Element Plus 的地图工具栏组件",
  "main": "./dist/ol-toolbar.umd.js",
  "module": "./dist/ol-toolbar.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/ol-toolbar.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/ol-toolbar.umd.js"
      },
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "set NODE_ENV=production && vite build",
    "preview": "vite preview",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "openlayers",
    "ol",
    "toolbar",
    "vue3",
    "element-plus",
    "map",
    "gis"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": ""
  },
  "packageManager": "pnpm@10.26.2",
  "peerDependencies": {
    "vue": "^3.5.0",
    "element-plus": "^2.8.0",
    "ol": "^8.2.0",
    "@element-plus/icons-vue": "^2.3.2"
  },
  "devDependencies": {
    "@types/node": "20.12.0",
    "@vitejs/plugin-vue": "5.0.0",
    "element-plus": "2.8.0",
    "ol": "8.2.0",
    "typescript": "5.9.3",
    "vite": "5.2.0",
    "vite-plugin-dts": "3.1.0",
    "vue": "3.5.0"
  }
}
